{
  "name": "napkin-mcp-unofficial",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Unofficial MCP server for Napkin AI tools",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "napkin-mcp-unofficial": "dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "doc": "tsx scripts/read-doc.ts",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.3.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.22.6"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "vitest": "^2.1.3"
  }
}
